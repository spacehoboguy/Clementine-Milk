{
  "name": "Clementine Milk",
  "author": "spacehoboguy",
  "dark": false,
  "editorScheme": "scheme/catppuccin_latte_modified.xml",
  "colors": {
    "parchment": "#FAEFD4",
    "tangerine" : "#EB8424",
    "black": "#000000",
    "white": "#ffffff",
    "success": "#67c751",
    "pending": "#5172c7",
    "error": "#c7516d",
    "warning": "#ffb730"
  },
  "ui": {

    "*": {
      "foreground": "tangerine",
      "background": "parchment",

      "infoForeground": "warning",
      "successForeground": "success",

      "errorForeground": "error",

      "selectionBackground": "tangerine",
      "selectionForeground": "#FFFFFF",

      "borderColor": "parchment",
      "focusColor": "tangerine",
      "focusedBorderColor": "tangerine",
      "hoverBorderColor": "tangerine",
      "pressedBorderColor": "tangerine",
      "hoverBackgroundColor": "parchment",
      "separatorColor": "tangerine",
      "lineSeparatorColor": "tangerine"
    },

    "Plugins": {
      "Tab": {
        "selectedForeground": "success",
        "selectedBackground": "error",
        "hoverBackground": "pending"
      }
    },

    "Editor" : {
      "shortcutForeground": "red",
      "background": "parchment",
      "foreground": "black",
      "Toolbar.borderColor": "parchment"
    },

    "EditorPane": {
      "splitBorder": "tangerine",
      "background": "tangerine",
      "foreground": "black"
    },

    "NavBar": {
      "borderColor": "parchment"
    },

    "MenuBar": {
      "selectionBackground": "red",
      "highlight": "salmon",
      "borderColor": "parchment"
    },

    "MainToolbar": {
      "background": "green",
      "separatorColor": "parchment"
    },

    "ToggleButton": {
      "onForeground": "#FFFFFF",
      "onBackground": "red",
      "offForeground": "#FFFFFF",
      "offBackground": "green",
      "buttonColor":  "#FFFFFF",
      "borderColor": "#FFFFFF"
    },

    "RunWidget" : {
      "foreground": "black",
      "runIconColor": "black",
      "iconColor": "black",
      "runningBackground": "#599e5e",
      "hoverBackground": "#8cea91",
      "pressedBackground": "#00000028"
    },

    "ProgressBar": {
      "trackColor": "#404040",
      "progressColor": "#FFFFFF",
      "indeterminateStartColor": "#FFFFFF",
      "indeterminateEndColor": "#1a1a1a",
      "failedColor": "error",
      "failedEndColor": "#431C27",
      "passedColor": "success",
      "passedEndColor": "#15451E"
    }
  },

  "icons": {
    "ColorPalette": {
      "Actions.Blue": "#5172c7",
      "Actions.Green": "#67c751",
      "Actions.Red": "#c7516d",
      "Actions.Grey": "#beb9b9"
    }
  }
}